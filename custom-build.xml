<?eclipse.ant.import?>
<project basedir="." default="build" name="oim">
 <target name="war" depends="build">
    <war destfile="build/oim.war" webxml="WebContent/WEB-INF/web.xml">
        <classes dir="build/classes"/>
        <fileset dir="WebContent">
            <exclude name="WEB-INF/web.xml"/>
        </fileset>
    </war>
  </target>
    <target name="deploy" depends="war">
        <copy file="build/oim.war" tofile="/usr/share/tomcat6/webapps/oim.war" overwrite="true"/>
    </target>
    </project>