<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>oim</display-name>
	<servlet>
		<description>
		</description>
		<display-name>
		KoukanServlet</display-name>
		<servlet-name>KoukanServlet</servlet-name>
		<servlet-class>
		com.koukan.test.KoukanServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		MainServlet</display-name>
		<servlet-name>MainServlet</servlet-name>
		<servlet-class>
		edu.iu.grid.oim.servlet.MainServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		DivexServlet</display-name>
		<servlet-name>DivexServlet</servlet-name>
		<servlet-class>
		com.webif.divex.DivexServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		HomeServlet</display-name>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>
		edu.iu.grid.oim.servlet.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		ErrorServlet</display-name>
		<servlet-name>ErrorServlet</servlet-name>
		<servlet-class>
		edu.iu.grid.oim.servlet.ErrorServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		VOServlet</display-name>
		<servlet-name>VOServlet</servlet-name>
		<servlet-class>
		edu.iu.grid.oim.servlet.VOServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KoukanServlet</servlet-name>
		<url-pattern>/koukan</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MainServlet</servlet-name>
		<url-pattern>/MainServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DivexServlet</servlet-name>
		<url-pattern>/divex</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ErrorServlet</servlet-name>
		<url-pattern>/error</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VOServlet</servlet-name>
		<url-pattern>/vo</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<error-page>
     <error-code>404</error-code>
     <location>/error</location>
   </error-page>
<error-page>
	<exception-type>java.lang.Exception</exception-type>
    <location>/error</location>
</error-page>

</web-app>
